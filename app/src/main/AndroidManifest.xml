<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TomatoComposeDemo"
        tools:targetApi="31">
        <activity
            android:name=".wei.WeiActivity"
            android:exported="false"
            android:label="@string/title_activity_wei"
            android:theme="@style/Theme.TomatoComposeDemo" />
        <activity
            android:name=".unit3state.ui.RecoverActivity"
            android:exported="true"
            android:label="@string/title_activity_recover"
            android:theme="@style/Theme.TomatoComposeDemo"></activity>
        <activity
            android:name=".unit3state.ui.TodoActivity"
            android:exported="true"
            android:label="@string/title_activity_todo"
            android:theme="@style/Theme.TomatoComposeDemo"></activity>
        <activity
            android:name=".unit2.WidgetActivity"
            android:exported="true"
            android:label="@string/title_activity_main"
            android:theme="@style/Theme.TomatoComposeDemo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".unit1.FirstListActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.TomatoComposeDemo" />
    </application>

</manifest>